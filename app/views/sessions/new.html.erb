<% if flash[:warning] %>
    <div class="notice"><%= flash[:warning] %></div>
<% end %>

<h1>Login</h1>
   <%= form_tag '/login' do %>
   <%= label_tag :username%>
   <%= text_field_tag :username %>
   <%= label_tag :password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<% end %>

<p>If you meant to click the sign up page, I've got you:</p>
<%= button_to "Sign Up", '/users/new', method: :get%>
